<?xml version="1.0"?>
<!DOCTYPE memspec SYSTEM "memspec.dtd">
<memspec>
  <comment>Based on datasheet 1GbDDR2.pdf – Rev. Z 03/14 EN, Die Revision H.
Corresponds to DDR2-800D JEDEC speed bin based on CL value.
Using timing for Micron -25E speed grade (CL = 5, clk = 400 MHz).
  </comment>
  <parameter id="memoryId"         value="MICRON_128MB_DDR2-800_16bit_H" />
  <parameter id="memoryType"       value="DDR2"                          />
  <parameter id="datasheet"        value="1GbDDR2.pdf – Rev. Z 03 14 EN.pdf"     />
  <parameter id="JEDEC"            value="JESD79-2E"                             />
  <parameter id="dieRevision"      value="H"                                     />
  <parameter id="speedBin"         value="DDR2-800D"                             />
  <parameter id="vendorSpeedGrade" value="-25E"                                  />
  <parameter id="capacity"         value="1Gb"                                   />
  <memarchitecturespec>
    <parameter id="burstLength"      value="8"                                   />
    <parameter id="dataRate"         value="2"                                   />
    <parameter id="nbrOfBanks"       value="8"                                   />
    <parameter id="nbrOfColumns"     value="1024"                                />
    <parameter id="nbrOfRanks"       value="1"                                   />
    <parameter id="nbrOfRows"        value="8192"                                />
    <parameter id="width"            value="16"                                  />
  </memarchitecturespec>
  <memtimingspec>
    <parameter id="AL"     type="uint"   value="0"                                                        />
    <parameter id="CCD"    type="uint"   value="2"                                                        />
    <parameter id="CKE"    type="uint"   value="3"                                                        />
    <parameter id="CKESR"  type="uint"   value="4"    comment="Using ISXR + CKE"                          />
    <parameter id="CL"     type="uint"   value="5"                                                        />
    <parameter id="clkMhz" type="double" value="400"                                                      />
    <parameter id="DQSCK"  type="uint"   value="0"                                                        />
    <parameter id="FAW"    type="uint"   value="18"                                                       />
    <parameter id="RAS"    type="uint"   value="16"                                                       />
    <parameter id="RC"     type="uint"   value="22"                                                       />
    <parameter id="RCD"    type="uint"   value="5"                                                        />
    <parameter id="REFI"   type="uint"   value="3120"                                                     />
    <parameter id="RFC"    type="uint"   value="51"                                                       />
    <parameter id="RP"     type="uint"   value="5"                                                        />
    <parameter id="RRD"    type="uint"   value="4"                                                        />
    <parameter id="RTP"    type="uint"   value="3"                                                        />
    <parameter id="WR"     type="uint"   value="6"                                                        />
    <parameter id="WTR"    type="uint"   value="3"                                                        />
    <parameter id="XP"     type="uint"   value="2"                                                        />
    <parameter id="XPDLL"  type="uint"   value="8"    comment="Using XARD (slow active power-down exit)"  />
    <parameter id="XS"     type="uint"   value="55"   comment="Using XSNR (self refresh exit to non read" />
    <parameter id="XSDLL"  type="uint"   value="200"  comment="Using XSR"                                 />
  </memtimingspec>
  <mempowerspec>
    <parameter id="idd0"   type="double" value="80.0"  />
    <parameter id="idd2n"  type="double" value="30.0"  />
    <parameter id="idd2p0" type="double" value="7.0"   />
    <parameter id="idd2p1" type="double" value="7.0"   />
    <parameter id="idd3n"  type="double" value="35.0"  />
    <parameter id="idd3p0" type="double" value="10.0"  />
    <parameter id="idd3p1" type="double" value="20.0"  />
    <parameter id="idd4r"  type="double" value="150.0" />
    <parameter id="idd4w"  type="double" value="160.0" />
    <parameter id="idd5"   type="double" value="150.0" />
    <parameter id="idd6"   type="double" value="7.0"   />
    <parameter id="vdd"    type="double" value="1.8"   />
  </mempowerspec>
</memspec>
